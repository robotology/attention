

<application>
	<name>Synchronous Image Acquisition </name>
 	<dependencies>
	</dependencies>
	
	<module>
		<name>dataDumper</name>
		<parameters>--name /channel1 --type image  --downsample 2 </parameters>
		<tag>ch1Dumper</tag>
		<node>/icub14</node>
	</module>
        <module>
		<name>dataDumper</name>
		<parameters>--name /channel2 --type image  --downsample 2 </parameters>
		<tag>ch2Dumper</tag>
		<node>/icub14</node>
	</module>
	<module>
		<name>dataDumper</name>
		<parameters>--name /channel3 --type image  --downsample 2 </parameters>
		<tag>ch3Dumper</tag>
		<node>/icub14</node>
	</module>
	<module>
		<name>dataDumper</name>
		<parameters>--name /channel4 --type image --downsample 2 </parameters>
		<tag>ch4Dumper</tag>
		<node>/icub14</node>
	</module>
	<module>
		<name>dataDumper</name>
		<parameters>--name /channel5 --type image  --downsample 2 </parameters>
		<tag>ch5Dumper</tag>
		<node>/icub14</node>
	</module>
	<module>
		<name>dataDumper</name>
		<parameters>--name /channel6 --type image --downsample 2 </parameters>
		<tag>ch6Dumper</tag>
		<node>/icub14</node>
	</module>
	
	<connection>
                <from>/yuvProc/icub/left_cam/UV/image:o</from>
		<to>/channel1</to>
		<protocol>tcp</protocol>
	</connection>
	<connection>
                <from>/hsvProc/icub/left_cam/H/image:o</from>
		<to>/channel2</to>
		<protocol>tcp</protocol>
	</connection>
	<connection>
                <from>/hsvProc/icub/left_cam/S/image:o</from>
		<to>/channel3</to>
		<protocol>tcp</protocol>
	</connection>
        <connection>
                <from>/hsvProc/icub/left_cam/V/image:o</from>
		<to>/channel4</to>
		<protocol>tcp</protocol>
	</connection>
	<connection>
                <from>/visualFilter/icub/left_cam/image:o</from>
		<to>/channel5</to>
		<protocol>tcp</protocol>
	</connection>
	<connection>
                <from>/blobFinder/icub/left_cam/salienceMap:o</from>
		<to>/channel6</to>
		<protocol>tcp</protocol>
	</connection>
   
</application>
